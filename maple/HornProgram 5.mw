<Group view="code" labelreference="L28" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">CheckMDCondition:=proc(P)
   local rule1, rule2,i,j;

   for i from 1 to nops(P)-1 do
     rule1:=P[i];
     for j from i+1 to nops(P) do
       rule2:=P[j];
       if rule1[1] &lt;&gt; rule2[1] then next;
       else
           if nops(rule1[2])&gt;1 or nops(rule2[2])&gt;1 then
              return 0;
           fi;
       fi;
     od;
   od;

   return 1;
end proc:</Text-field>
</Input>
</Group>
