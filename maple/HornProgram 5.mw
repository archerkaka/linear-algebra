<Group view="code" labelreference="L38" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">SubSet:=proc(A,B)
  local t,u,flag;

  for t in A do
    flag:=0;
    for u in B do
     if t = u then
         flag:=1;
         break;
     fi;
    od;
   
    if flag = 0 then return false fi;
  od;

  return true;
end proc:</Text-field>
</Input>
