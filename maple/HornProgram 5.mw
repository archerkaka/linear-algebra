<Group labelreference="L20" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">FindFixPoint:=proc(List,P,Base,n)
   local v0,v,u,n1,k,M,
         M1,M2,i,j,
         t1,t2,
         compare;

   n1:=nops(Base);

 
   compare:=proc(u,v)
     local i;
     for i from 1 to n1 do
        if u[i]&lt;&gt;v[i]then return 0; fi;
     od;
     return 1;
   end:

print(&quot;Matrix&quot;);
t1:=time();
   M:=CreateMatrix(n1,P,Base);
print(time()-t1);

   M1:=Matrix(n,n1);
   for i from 1 to n do
      for j from 1 to n1 do
         M1[i,j]:=M[i,j];
      od;
   od:

   M2:=Matrix(n+1,n1-n);
   for j from 1 to n1-n do
      for i from 1 to n do
         if M[n+j,i]&lt;&gt;0 then 
              M2[i,j]:=-1;
         fi;
      od;
      M2[n+1,j]:=add(M2[i,j],i=1..n); 
   od:

   v0:=VectorRepresent(n1,List,Base);
   v:=v0;

print(&quot;Product&quot;);
t1:=time();
   u,M2:=ProductOper(n,n1,M1,M2,v,v0);
   while compare(u,v)&lt;&gt;1 do
      v:=u;
      u,M2:=ProductOper(n,n1,M1,M2,v,v0); 
   od;
print(time()-t1);

   v:=v0;

print(&quot;Product1&quot;);
t1:=time();
   u:=ProductOper1(n1,M,v,v0);
   while compare(u,v)&lt;&gt;1 do
#printf(&quot;u[6]=%d&quot;,u[6]);
      v:=u;
      u:=ProductOper1(n1,M,v,v0); 
   od;
print(time()-t1);

   return u;
   
end proc:</Text-field>
</Input>
</Group>
