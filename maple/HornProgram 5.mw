<Group view="code" labelreference="L3" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">CreateMatrix:=proc(n,P,Base)
  local M, rule, value, varia,
        i,j;

  M:= Matrix(n);
#  if member(L,Base,'j') then
#     for i from 1 to n do
#        M[i,j] := 1;
#     od;
#  fi;
     
  for i from 1 to n do
#    if Base[i] = T then
#      for j from 1 to n do
#        M[i,j] := 1;
#      od;
#      next;
#    fi;

    for rule in P do
       if Base[i] in rule[1] then
         if rule[2] &lt;&gt; {} then
            value:=evalf(1/nops(rule[2]));
         else
            next;
         fi;

         for varia in rule[2] do
            member(varia,Base,'j');
            M[i,j]:=value;
         od;
       fi;
    od;
  od;

  return M;
end proc:</Text-field>
</Input>
</Group>
