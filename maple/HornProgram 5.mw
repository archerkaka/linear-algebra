<Group view="code" labelreference="L18" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">ProductOper:=proc(n,n1,M1,M2,v,v0)
   local result,temp,Temp,i,j;

   Temp:=M2;
CUDA:-Enable(true):
   temp:=M1.v;
CUDA:-Enable(false):
   result:=Vector(n1);
   for i from 1 to n do
      result[i]:=temp[i];
   od;
   result:=result+v0;


#printf(&quot;result[6]=%d&quot;,result[6]);
   for i from 1 to n1 do
      if result[i]&gt;=1 then result[i] := 1;
      else result[i]:=0;
      fi;
   od;

   for i from 1 to n do
      if result[i] = 1 then
         for j from 1 to n1-n do
             if Temp[i,j] = -1 then Temp[i,j]:=1; fi;
         od;
      fi;
   od;

   for j from 1 to n1-n do
      if add(Temp[i,j],i=1..n) = -Temp[n+1,j] then
         result[n+j]:=1;
      fi;
   od;

   return result,Temp;
end proc:</Text-field>
</Input>
</Group>
