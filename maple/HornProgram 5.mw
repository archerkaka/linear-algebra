<Group labelreference="L25" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">CreateProgram:=proc(n,m)
  local k,i,j,t,List,r,temp,tam,CountNum,tong,
        P,P1,H,Supp,Check,DS,head,body;


  for t from 1 to 10 do
    k:=rand() mod trunc(n/3) + 1;
  od;

  if (m-k) &gt;= 50 then
     if trunc((m-k)*0.04) &gt; n*n then
         DS:=[k,n*n/2,trunc((m-k)*0.04),trunc((m-k)*0.1),trunc((m-k)*0.4),trunc((m-k)*0.35),trunc((m-k)*0.04),trunc((m-k)*0.02)];
     else

         DS:=[k,trunc((m-k)*0.04),trunc((m-k)*0.04),trunc((m-k)*0.1),trunc((m-k)*0.4),trunc((m-k)*0.35),trunc((m-k)*0.04),trunc((m-k)*0.02)];
     fi;

     DS:=[op(DS),m-sum(DS[i],i=1..8)];
  else
    DS:=[k,1,1,1,trunc((m-k)*0.4),trunc((m-k)*0.35),1,1];
    DS:=[op(DS),m-sum(DS[i],i=1..8)];
  fi;
#  List:= Array([seq(0,i=1..DS[1]),seq(1,i=1..DS[2]),seq(2,i=1..DS[3]),seq(3,i=1..DS[4]),seq(4,i=1..DS[5]),seq(5,i=1..DS[6]),seq(6,i=1..DS[7]),seq(7,i=1..DS[8]),seq(8,i=1..DS[9])]);

  
  print(DS);
  tong:=Array([seq(0,i=1..n)]);

  head:=rand() mod n + 1;
  r:= [{x[head]},{T}];

  P:={r};
  Check:={r};
  tong[head]:=tong[head]+1;

  temp:=1;
  CountNum:=DS[temp];
  H:={x[head]};
  for i from 2 to (DS[1]+DS[2]) do
#   printf(&quot;i = %d&quot;,i);

    if i &gt; CountNum then
#<Font encoding="UTF-8">       printf(&quot;k = %d\134n&quot;, temp-1);</Font>
       temp:=temp+1;
       CountNum:=CountNum + DS[temp];
       Check:={P[1]};
    fi;

    head:=rand() mod n + 1;
    while tong[head]&gt;=n-1 do
        head:=rand() mod n + 1;
    od;
    k:=temp-1;

    if k = 0 then
         r:=[{x[head]},{T}];
         while r in Check do
            head:=rand() mod n + 1;
            r:=[{x[head]},{T}];
         od;    
         H:=H union {x[head]};
    elif k = 1 then
         r:=Check[1];
         while r in Check do    <Font foreground="[51,204,204]">
   </Font>         body:=rand() mod n + 1;
            while (body = head) do          
   <Font foreground="[51,204,204]">   </Font>         body:=rand() mod n + 1;
            od;
            r:= [{x[head]},{x[body]}];
         od;    
    fi;

    P:=P union {r};
    Check:=Check union {r};
    tong[head]:=tong[head]+1;

  od;

#<Font encoding="UTF-8">printf(&quot;\134nSecond \134n&quot;);</Font>

  Supp:={};
  for i from 1 to DS[1] do
     Supp:=Supp union {[{H[i]},{T}]};
  od;

  P1:=P minus Supp;
  Supp:={};  
  tam:=1;
  for i from (DS[1]+DS[2]+1) to m do
#printf(&quot;i = %d&quot;,i);
    if i &gt; CountNum then
#<Font encoding="UTF-8">       printf(&quot;k = %d\134n&quot;, temp-1);
</Font>       temp:=temp+1;
       CountNum:=CountNum + DS[temp];
       Check:={P[1]};
    fi;

    head:=rand() mod n + 1;
    k:=temp-1;

    r:=Check[1];
    while r in Check do    <Font foreground="[51,204,204]">
</Font>        body:={};
        for j from 1 to k do          
             t:=head;
             while (t = head) or (t in body) do
                t:=rand() mod n + 1;
             od;
             body:=body union {t};
        od;
        body:=seq(x[j], j in body);
        r:= [{x[head]},{body}];
    od;
        
    Supp:= Supp union {[{x[head]},{x[n+tam]}],[{x[n+tam]},{body}]};
    tam:=tam+1;

    P:=P union {r};
    Check:=Check union {r};

  od;

  P1:=P1 union Supp;

  return P,P1,H; #GTLN,TB,DoLech;

end:</Text-field>
