<Group view="code" hide-output="false" labelreference="L9" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">CreateMatrix:=proc(n,P,Base)
  local M, rule, value, varia,
        i,j;

  M:= Matrix(2*n);
  
  for rule in P do
    member(rule[1][1],Base,'i');
    if type(rule[2],`list`) then
       for varia in rule[2] do
          member(varia,Base,'j');
          M[i,j]:=1;
       od;

    else
       if rule[2] &lt;&gt; {} then
            value:=1/nops(rule[2]);
       else
            member(rule[1][1],Base,'j');
            M[j,j]:=1;
       fi;

       for varia in rule[2] do
          if member(varia,Base,'j') then
               M[i,j]:=value;
          elif member(-varia,Base,'j') then
               M[i,n+j]:=value;
          fi;
     
       od;
    fi;
  od;

  for i from 1 to n do
     M[n+i,n+i]:=1;
  od;

  return M;
end proc:</Text-field>
</Input>
</Group>
