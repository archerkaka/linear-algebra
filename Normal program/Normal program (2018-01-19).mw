<Group view="code" hide-output="false" labelreference="L44" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">TOp_LeastModel:=proc(P,Base)
  local result,u,
        v0,v,t,rule;

  v:={};
  if nargs = 3 then
     u:= args[3] union {seq(-t,t in Base)};
  else
     u:={seq(-t,t in Base)};
  fi;

  while u &lt;&gt; v do
    v:=u;
    for rule in P do
      if SubSet(rule[2],v) then
        u:= u union rule[1];
      fi;
    od;
  od;

  result:={};
  for t in u do
    if t in Base and (-t in u) then
        result:=result union {t};
    fi;
  od;
  return result;
end:</Text-field>
</Input>
