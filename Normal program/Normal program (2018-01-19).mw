<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">VectorFacts:=proc(n,P,Base)
   local v,rule,t;

   v:=Vector(n);
   if nargs = 3 then
     for rule in P do
       if rule[2]={} then
         member(rule[1][1],Base,'t');
         v[t]:=1;   
       fi;   
     od;
