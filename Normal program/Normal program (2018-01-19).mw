<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">TransferToSDProgram:=proc(n, P, H, NumberHead)
   local P1, Base1,
         Supp,NumHead,Base,i,j,rule,current,temp;

   Base:={seq(x[i],i=1..n)};
   NumHead:=NumberHead;
   Supp:={};

   for rule in P do
     member(rule[1][1],Base,'i');
     if x[i] in H then
        NumHead[i]:=NumHead[i]-1;
     else
        Supp:=Supp union {rule};
     fi;
   od;



   P1:={};

   current:=0;
   temp:=0;
   for i from 1 to n do
      if NumHead[i] = 1 then
        P1:=P1 union {Supp[current+1]};

      elif NumHead[i] &gt; 1 then
        rule:=[{x[i]},[seq(x[n+temp+k],k=1..NumHead[i])]];
        P1:=P1 union {rule};

        for j from 1 to NumHead[i] do
           rule:=[{x[n+temp+j]},Supp[current+j][2]];
           P1:=P1 union {rule};           
        od;

        temp:=temp+NumHead[i];
      fi;

      current:=current + NumHead[i];

   od;

   Base1:={seq(x[i],i=1..n+temp)};
   return P1,Base1;
 
end:</Text-field>
</Input>
</Group>
