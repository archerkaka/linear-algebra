<Group view="code" hide-output="false" labelreference="L45" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">CreateMatrix2:=proc(n1,P1,Base1,n,H)
  local M1,M2,u,
        M,v,rule,varia,value,i,j,k,t;

  M:=Matrix(n1,n);
  M2:=Matrix(n,n1-n);
  v:=Vector(n1);
  k:=0;

  for rule in P1 do
    member(rule[1][1],Base1,'i');
    if type(rule[2],`list`) then
       for varia in rule[2] do
          member(varia,Base1,'j');
          M2[i,j-n]:=-1;
       od;

    else
       if rule[2] &lt;&gt; {} then
            value:=1/nops(rule[2]);
            v[i]:=1;
            k:=k+1;
       else
            member(rule[1][1],Base1,'j');
            if j &lt;= n then
               M[j,j]:=1;
               v[j]:=1;
               k:=k+1;
            fi;
       fi;

       for varia in rule[2] do
            member(varia,Base1,'j');
            M[i,j]:=value;
       od;
    fi;
  od;

  i:='i';

  M1:=Matrix(k,n);
  u:=Vector(k);
  i:=0;
  for t from 1 to n1 do
    if v[t] = 1 then
       i:=i+1;
       u[i]:=t;
       for j from 1 to n do
         M1[i,j]:=M[t,j];
       od;
    fi;
  od;

  return M1,M2,u;

end proc:</Text-field>
</Input>
</Group>
